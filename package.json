{
  "name": "k-store",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none npm run dev:client\" \"npm run dev:api\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:client": "npm run start --prefix client",
    "dev:api": "npm run start --prefix api",
    "build": "npm run build --prefix client",
    "package": "npm run build && electron-packager . KStore --platform=win32 --arch=x64 --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.2.2",
    "wait-on": "^9.0.1"
  },
  "devDependencies": {
    "electron-packager": "^17.1.2"
  }
}
